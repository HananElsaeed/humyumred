<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hananelsaid.hp.humyumred">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash_package.Splash_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Home_package.View.Home_Activity"
            android:label="@string/title_activity_home_"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ContactUs_pakage.View.Contact_Activity"
            android:label="@string/title_activity_contact"
            android:parentActivityName=".Home_package.View.Home_Activity" />
        <activity
            android:name=".Terms_package.TermsActivity"
            android:parentActivityName=".Home_package.View.Home_Activity" />
        <activity android:name=".User_Login_package.View.Login_Activity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>